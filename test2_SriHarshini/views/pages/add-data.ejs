<form action="/api/add-data" method="POST">
    <label>Course Name: <input type="text" name="course_name" required></label><br>
    <label>Course Code: <input type="text" name="course_code" required></label><br>
    <label>Instructor Name: <input type="text" name="instructor_name" required></label><br>
    <label>Start Date: <input type="date" name="start_date" required></label><br>
    <label>Credits: <input type="number" name="credits" step="0.1" required></label><br>
    <button type="submit">Submit</button>
</form>
